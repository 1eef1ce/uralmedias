						.progress__item
							.progress__num 10
							.progress__text сертифицированных специалистов
						.progress__item
							.progress__num 300
							.progress__text разработанных проектов
						.progress__item
							.progress__num 9
							.progress__text лет мы создаем сайты
						.progress__item
							.progress__num 100
							.progress__text миллионов строк кода

		.team
			.team__container.container
				.team__title.title-3 Наша команда
				.team__cover
					.team__items
						.team__item
							.team__pic(style="background-image: url(./img/team/p1.png);")
