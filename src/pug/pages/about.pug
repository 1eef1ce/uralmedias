extends ../layout/main

block variables
	- var title = 'Главная страница'
	- var pageClass = 'index-page';
	- var setTitle = 'МЫ URALMEDIAS';
	- var include = 'btn';
	- var quote = 'about';
	- var bildboard_img = "background-image: url(img/dataBild/img1.jpg);"

block content
